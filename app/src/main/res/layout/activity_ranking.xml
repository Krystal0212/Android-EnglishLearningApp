                        android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_300pxh">
                        <FrameLayout android:id="@+id/frameStacktoptwo"
                            android:layout_gravity="top|end"
                            android:layout_height="@dimen/_75pxv"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_95pxh">
                            <ImageView android:id="@+id/imageTopTwo"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_75pxh"
                                android:layout_width="@dimen/_95pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_top2"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_top2"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSecondBox"
                                android:layout_gravity="bottom|start"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_29pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="wrap_content"
                                android:paddingBottom="@dimen/_4pxv"
                                android:paddingEnd="@dimen/_6pxh"
                                android:paddingStart="@dimen/_7pxh"
                                android:paddingTop="@dimen/_5pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_2"
                                style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_2"/>
                        </FrameLayout>
                        <ImageView android:id="@+id/imageTopOne"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_65pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_101pxh"
                            android:layout_marginStart="@dimen/_101pxh"
                            android:layout_width="@dimen/_89pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_top1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_top1"/>
                        <FrameLayout
