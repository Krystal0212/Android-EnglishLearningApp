                                android:layout_gravity="start"
                                android:layout_height="@dimen/_87pxh"
                                android:layout_width="@dimen/_103pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_top3"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_top3"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtThirdBox"
                                android:layout_gravity="bottom|end"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_29pxh"
