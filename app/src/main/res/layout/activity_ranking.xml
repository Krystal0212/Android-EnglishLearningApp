                                android:layout_width="wrap_content"
                                android:paddingBottom="@dimen/_4pxv"
                                android:paddingEnd="@dimen/_6pxh"
                                android:paddingStart="@dimen/_7pxh"
                                android:paddingTop="@dimen/_4pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_3"
                                style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_3"/>
                        </FrameLayout>
                        <ImageView android:id="@+id/imageLinkRopeOne"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginBottom="@dimen/_27pxv"
                            android:layout_marginEnd="@dimen/_85pxh"
                            android:layout_marginStart="@dimen/_85pxh"
                            android:layout_marginTop="@dimen/_27pxv"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_linkrope1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_linkrope1"/>
                        <ImageView android:id="@+id/imageLinkRopeTwo"
                            android:layout_gravity="top|end"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginBottom="@dimen/_39pxv"
                            android:layout_marginEnd="@dimen/_82pxh"
                            android:layout_marginStart="@dimen/_82pxh"
                            android:layout_marginTop="@dimen/_39pxv"
                            android:layout_width="@dimen/_38pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_linkrope2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_linkrope2"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFirstBox"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_45pxv"
                            android:layout_marginEnd="@dimen/_136pxh"
                            android:layout_marginStart="@dimen/_136pxh"
                            android:layout_marginTop="@dimen/_45pxv"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_5pxv"
                            android:paddingEnd="@dimen/_9pxh"
                            android:paddingStart="@dimen/_8pxh"
                            android:paddingTop="@dimen/_4pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_1"
                            style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_1"/>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
            <View android:background="@color/black_900"
                android:id="@+id/lineLineTwenty"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_272pxh"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerRanking"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_ranking"/>
            <View android:background="@color/black_900"
                android:id="@+id/lineLineTwentyThree"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_28pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="@dimen/_272pxh"/>
