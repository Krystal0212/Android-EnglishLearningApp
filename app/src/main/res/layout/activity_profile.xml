<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout android:gravity="start"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_4pxh"
        android:layout_marginStart="@dimen/_11pxh"
        android:layout_marginTop="@dimen/_16pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_deep_orange_200_border_black_900_radius_12"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft_black_900" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_86pxh"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_my_profile"
                    style="@style/txtOpenSansromansemibold24" tools:text="@string/lbl_my_profile"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnrectangle2161"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameStackusername"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_567pxv"
                    android:layout_marginEnd="@dimen/_8pxh" android:layout_width="@dimen/_337pxh">
                    <LinearLayout android:id="@+id/linearColumnusername"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecyan_50cornerRadius_1">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtUsername"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_39pxh"
                            android:layout_marginStart="@dimen/_39pxh"
                            android:layout_marginTop="@dimen/_74pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_username"
                            style="@style/txtOpenSansromansemibold20_1" tools:text="@string/lbl_username"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDisplayname"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_45pxh"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_display_name"
                            style="@style/txtOpenSans16" tools:text="@string/lbl_display_name"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_white_a700_border_gray_901_radius_12"
                            android:drawableEnd="@drawable/drawable_img_group_3694"
                            android:drawablePadding="@dimen/_30pxh"
                            android:ellipsize="end"
                            android:hint="@string/lbl_username2"
                            android:id="@+id/etCyanCard"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_39pxh"
                            android:layout_marginStart="@dimen/_39pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:paddingBottom="@dimen/_11pxv"
                            android:paddingEnd="@dimen/_10pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_14pxv"
                            android:singleLine="true"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRoundedOutline_5" tools:ignore="Autofill"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEmail"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_44pxh"
                            android:layout_marginStart="@dimen/_44pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_email"
                            style="@style/txtOpenSans16" tools:text="@string/lbl_email"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_white_a700_border_gray_901_radius_12"
                            android:drawableEnd="@drawable/drawable_img_group_3695"
                            android:drawablePadding="@dimen/_30pxh"
                            android:ellipsize="end"
                            android:hint="@string/lbl_a_gmail_com"
                            android:id="@+id/etEmailOne"
                            android:inputType="textEmailAddress"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_39pxh"
                            android:layout_marginStart="@dimen/_39pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:paddingBottom="@dimen/_11pxv"
                            android:paddingEnd="@dimen/_10pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_14pxv"
                            android:singleLine="true"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearChangePassword"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_139pxv"
                            android:layout_marginEnd="@dimen/_39pxh"
                            android:layout_marginStart="@dimen/_39pxh"
                            android:layout_marginTop="@dimen/_46pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStyleamber_A100cornerRadius">
                            <ImageView android:id="@+id/imageVector"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginStart="@dimen/_15pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtChangePassword"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginEnd="@dimen/_75pxh"
                                android:layout_marginStart="@dimen/_48pxh"
                                android:layout_marginTop="@dimen/_16pxv"
                                android:layout_weight="0.85"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_change_password"
                                style="@style/txtOpenSansromansemibold12" tools:text="@string/lbl_change_password"/>
                        </LinearLayout>
                    </LinearLayout>
                    <FrameLayout
                        android:id="@+id/frameStackrectangle2161"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_125pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_104pxh"
                        android:layout_marginStart="@dimen/_104pxh"
                        android:layout_width="@dimen/_126pxh" style="@style/groupStylewhite_A700cornerRadius_2">
                        <ImageView android:id="@+id/imageAvatar"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_113pxh"
                            android:layout_marginHorizontal="@dimen/_5pxh"
                            android:layout_marginVertical="@dimen/_5pxv"
                            android:layout_width="@dimen/_115pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectangle2161"/>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowvectorOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleamber_A100cornerRadius">
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChangePasswordOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_92pxh"
                        android:layout_marginStart="@dimen/_52pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.89"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_9pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_change_password"
                        style="@style/txtOpenSansromansemibold18" tools:text="@string/lbl_change_password"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
