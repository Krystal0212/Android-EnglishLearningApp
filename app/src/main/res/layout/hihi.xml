<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearRanking"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearTop3"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_174pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelight_blue_100cornerRadius">
            <FrameLayout android:id="@+id/frameStackrectangle2229"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_188pxv"
                android:layout_width="@dimen/_330pxh"
                android:layout_marginEnd="@dimen/_37pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_8pxv">
                <ImageView android:id="@+id/imageCongratulation"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_81pxh"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_150pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle2229}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle2229"/>
                <LinearLayout android:id="@+id/linearRankListAvata"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <ImageView android:id="@+id/imageCharmIcon"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginEnd="@dimen/_119pxh"
                        android:layout_marginStart="@dimen/_110pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_charmicon"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_charmicon"/>
                    <LinearLayout android:id="@+id/linearRowavatar"
                        android:layout_height="170dp"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <FrameLayout android:id="@+id/frameStackavatar"
                            android:layout_height="@dimen/_110pxh"
                            android:layout_width="@dimen/_110pxh"
                            android:layout_marginTop="@dimen/_5pxh">
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv">
                                <ImageView android:id="@+id/imageTopThree"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_93pxh"
                                    android:layout_width="@dimen/_110pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_top3"
                                    tools:ignore="ContentDescription"
                                    tools:src="@drawable/img_top3"/>

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/viewAvatar"
                                    android:layout_width="@dimen/_45pxh"
                                    android:layout_height="@dimen/_45pxh"
                                    android:layout_centerInParent="true"
                                    android:background="@drawable/img_fluffy"
                                    android:rotation="-15.38" />

                                <TextView
                                    android:id="@+id/txtThirdText"
                                    style="@style/txtSolidRoundedOutline"
                                    android:layout_width="@dimen/_15pxh"
                                    android:layout_height="@dimen/_15pxh"
                                    android:layout_alignParentBottom="true"
                                    android:layout_gravity="center"
                                    android:layout_marginStart="@dimen/_46pxh"
                                    android:layout_marginTop="@dimen/_15pxh"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_alignBottom="@+id/viewAvatar"
                                    android:gravity="center"
                                    android:rotation="-15.38"
                                    android:text="@string/lbl_3"
                                    tools:text="@string/lbl_3" />
                            </RelativeLayout>

                        </FrameLayout>

                        <FrameLayout
                            android:layout_marginBottom="@dimen/_20pxh"
                            android:id="@+id/frameStackavatarOne"
                            android:layout_height="@dimen/_106pxv"
                            android:layout_width="@dimen/_106pxh">
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv">
                                <ImageView android:id="@+id/imageTopOne"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_89pxh"
                                    android:layout_width="@dimen/_106pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_top1"
                                    tools:ignore="ContentDescription"
                                    tools:src="@drawable/img_top1"/>

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/viewAvatarOne"
                                    android:layout_width="@dimen/_60pxh"
                                    android:layout_height="@dimen/_60pxh"
                                    android:layout_centerInParent="true"
                                    android:background="@drawable/img_fluffy" />

                                <TextView
                                    android:id="@+id/txtFirstBox"
                                    android:layout_alignBottom="@+id/imageTopOne"
                                    android:layout_height="@dimen/_15pxh"
                                    android:layout_width="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_37pxh"
                                    android:layout_marginEnd="@dimen/_37pxh"
                                    android:gravity="center"
                                    android:text="@string/lbl_1"
                                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_1"/>
                            </RelativeLayout>



                        </FrameLayout>

                        <FrameLayout
                            android:id="@+id/frameStackavatarTwo"
                            android:layout_height="@dimen/_90pxv"
                            android:layout_marginBottom="@dimen/_27pxv"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="@dimen/_90pxh">
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv">
                                <ImageView android:id="@+id/imageTopTwo"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_85pxh"
                                    android:layout_width="@dimen/_108pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_top2"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_top2"/>

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/viewAvatarTwo"
                                    android:layout_width="@dimen/_50pxh"
                                    android:layout_height="@dimen/_50pxh"
                                    android:layout_centerInParent="true"
                                    android:layout_gravity="center"
                                    android:background="@drawable/img_fluffy"
                                    android:rotation="11.18" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/txtSecondText"
                                android:layout_gravity="bottom|start"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_marginEnd="@dimen/_13pxh"
                                android:layout_marginStart="@dimen/_27pxh"
                                android:layout_marginBottom="@dimen/_8pxv"
                                android:layout_width="@dimen/_15pxh"
                                android:rotation="11.18"
                                android:gravity="center"
                                android:singleLine="true"
                                android:text="@string/lbl_2"
                                style="@style/txtSolidRoundedOutline"
                                tools:text="@string/lbl_2"/>
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <View android:background="@color/black_900"
                android:id="@+id/lineLineTwenty"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_272pxh"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerRanking"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_ranking"/>
            <View android:background="@color/black_900"
                android:id="@+id/lineLineTwentyThree"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_28pxv"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_272pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
