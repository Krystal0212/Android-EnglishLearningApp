<?xml version="1.0" encoding="UTF-8"?>
    <LinearLayout android:id="@+id/linearRowprofileimglarg"
        xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageProfileImgLarg"
            android:layout_height="@dimen/_50pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_50pxh"
            android:scaleType="centerCrop" circular="@{true}"
            android:src="@drawable/img_profileimglarg"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnrosejhenry"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_178pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_weight="0.69" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtRoseJHenry"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@string/lbl_rose_j_henry"
                style="@style/txtGilroysemibold18" tools:text="@string/lbl_rose_j_henry"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@string/lbl_6_15_pm"
                style="@style/txtGilroyregular14" tools:text="@string/lbl_6_15_pm"/>
        </LinearLayout>
    </LinearLayout>
