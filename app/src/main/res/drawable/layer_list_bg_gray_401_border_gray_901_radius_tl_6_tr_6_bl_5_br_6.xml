<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#c4c4c4"/>
            <corners android:bottomLeftRadius="@dimen/_5pxh"
                android:bottomRightRadius="@dimen/_6pxh"
                android:topLeftRadius="@dimen/_6pxh" android:topRightRadius="@dimen/_6pxh"/>
            <stroke android:color="@color/gray_901" android:width="@dimen/_1pxh"/>
        </shape>
    </item>
    <item>
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/yellow_600"/>
                <corners android:radius="@dimen/_5pxh"/>
                <stroke android:color="@color/gray_901" android:width="@dimen/_1pxh"/>
            </shape>
        </clip>
    </item>
</layer-list>
